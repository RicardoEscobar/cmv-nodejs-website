<%- (path==='/inscripciones' || path==='/contacto' ) && isEmailSuccessful
  ? '<script src="javascripts/notification.js"></script>' : '' %>
<% if (path==='/pilar' ) { %>
<script src="javascripts/initialCapitalization.js"></script>
<script>
  function capitalizeHeadings() {
    let h3Tags = document.getElementsByTagName("h3");
    for (let i = 0; i < h3Tags.length; i++) {
      h3Tags[i].textContent = initialCapitalization(h3Tags[i].textContent, ["de"]);
    }
  }
  window.onload = capitalizeHeadings;
</script>
<% } else { %>
<% } %>